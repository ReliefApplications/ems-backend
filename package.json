{
  "name": "oort-backend",
  "version": "1.4.0-beta.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=./build node build/src/index.js",
    "build": "tsc -p tsconfig.json && tsconfig-replace-paths -p tsconfig.json",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest --forceExit",
    "prepare": "husky install",
    "check-i18n": "node .scripts/check-i18n",
    "migrate": "migrate --migrations-dir=\"./migrations\" --compiler=\"ts:./src/utils/migrations/ts-compiler.js\"",
    "migrate:create": "migrate create --template-file ./src/utils/migrations/template.ts --migrations-dir=\"./migrations\" --compiler=\"ts:./src/utils/migrations/ts-compiler.js\"",
    "migrate:up": "migrate --migrations-dir=\"./migrations\" --compiler=\"ts:./src/utils/migrations/ts-compiler.js\" up",
    "migrate:down": "migrate --migrations-dir=\"./migrations\" --compiler=\"ts:./src/utils/migrations/ts-compiler.js\" down",
    "migrate:list": "migrate --migrations-dir=\"./migrations\" --compiler=\"ts:./src/utils/migrations/ts-compiler.js\" list",
    "semantic-release": "semantic-release"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "src/**/*.graphql"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "node --inspect=0.0.0.0:9229 --nolazy -r ts-node/register"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.5.0",
    "@casl/ability": "^5.2.2",
    "@casl/mongoose": "^5.0.0",
    "@ucast/mongo2js": "^1.3.3",
    "amqplib": "^0.8.0",
    "apollo-datasource-rest": "^3.5.2",
    "apollo-server-caching": "^3.3.0",
    "apollo-server-core": "^2.25.2",
    "apollo-server-express": "^2.19.0",
    "body-parser": "^1.20.1",
    "bson": "^4.2.3",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "cron-validator": "^1.3.1",
    "crypto-js": "^4.0.0",
    "dotenv": "^10.0.0",
    "email-templates": "^10.0.1",
    "events": "^3.3.0",
    "exceljs": "^4.2.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.2.1",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.3",
    "express-winston": "^4.2.0",
    "graphql": "^14.1.1",
    "graphql-amqp-subscriptions": "^1.1.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^11.0.0",
    "http": "0.0.1-security",
    "i18next": "^21.6.13",
    "i18next-http-middleware": "^3.2.0",
    "i18next-node-fs-backend": "^2.1.3",
    "inflection": "^1.12.0",
    "json2csv": "^5.0.6",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "migrate": "^1.8.0",
    "mime-types": "^2.1.33",
    "mongoose": "^5.13.15",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.4",
    "passport-keycloak-bearer": "^2.1.0",
    "pug": "^3.0.2",
    "sanitize-filename": "^1.6.3",
    "subscriptions-transport-ws": "^0.9.18",
    "survey-knockout": "^1.9.64",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "xhr2": "^0.2.1"
  },
  "devDependencies": {
    "@azure/msal-node": "^1.15.0",
    "@faker-js/faker": "^7.6.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/amqplib": "^0.8.0",
    "@types/config": "^3.3.0",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.9",
    "@types/express-fileupload": "^1.2.2",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/http-proxy": "^1.17.8",
    "@types/inflection": "^1.5.28",
    "@types/jest": "^29.2.3",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.171",
    "@types/mime-types": "^2.1.1",
    "@types/mongoose": "^5.10.5",
    "@types/passport-azure-ad": "^4.3.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-replace-paths": "^0.0.11",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReliefApplications/oort-backend.git"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": false
        }
      ],
      "@semantic-release/git"
    ]
  }
}
